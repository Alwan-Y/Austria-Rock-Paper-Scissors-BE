{
  "name": "austria-be",
  "version": "1.0.0",
  "description": "binar platinum class challenge BE project",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon src --exec babel-node",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "yarn clean && yarn build-babel",
    "start": "yarn build && node ./build",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:public:fix": "eslint public/ --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sequelize": "sequelize",
    "db:migrate": "sequelize db:migrate",
    "db:seed:all": "sequelize db:seed:all",
    "db:migrate:refresh": "sequelize db:migrate:undo:all && yarn db:migrate",
    "db:refresh:all": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "db:setup": "yarn sequelize db:create && yarn sequelize db:migrate && yarn sequelize db:seed:all"
  },
  "author": "Austria",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "firebase-admin": "^9.4.2",
    "pg": "^8.5.0",
    "sequelize": "^6.3.5",
    "socket.io": "^3.1.1",
    "swagger-ui-express": "^4.1.6",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0"
  }
}
